page{
  background: #f5f5f5;
}
.fiendList{
  width: 750rpx;
  background: #fff;
  border-top: 4rpx solid #f5f5f5;
}
.fiendList_fiends{
  position: relative;
  width: 720rpx;
  height: 100rpx;
  background: #fff;
  border-bottom: 2rpx solid #e1e0e1;
  margin-left: 30rpx;
  font-size: 30rpx;
  color: #36353A;
  line-height: 100rpx;
}
.fiendList_fiends_image image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.fiendList_fiends_image{
  position: absolute;
  top: 0;
  right: 260rpx;
  display: flex;
  flex-direction: row;
  align-items: center;

}
.fiendList_fiends_btn{
  width: 200rpx;
  height: 88rpx;
  position: absolute;
  top: 0;
  right: 30rpx;
}
.radio-group{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0rpx;
  right: 30rpx;
}
.fiendList_fiends_btn radio{
  margin-left: 10rpx;
}
button {
  margin-top: 290rpx;
width: 640rpx;
height: 80rpx;
position:relative;
display:block;
margin-left:auto;
margin-right:auto;
padding-left:14px;
padding-right:14px;
box-sizing:border-box;
font-size: 34rpx;
text-align:center;
text-decoration:none;
line-height: 80rpx;
border-radius:50rpx;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#000000;
background-color:#F8F8F8;
}
button[type=warn] {
color:#FFFFFF;
background-color:rgba(193,92,62,1);
}
radio {
  transform:scale(0.7);
}