page{
  width: 100%;
  height: 100%;
  border-top: 4rpx solid #f5f5f5;
}
.noMore-text{
  text-align: center;
  line-height: 500rpx;
  font-size: 30rpx;
}
.leftLine{
  position: fixed;
  left: 0;
  top: 0;
  width: 128rpx;
  height: 100%;
  border-right: 2rpx solid #D8D8D8;
  z-index: -1;
}
.box{
  width: 750rpx;
  min-height: 100rpx;
}
.box_year{
  width: 120rpx;
  height: 120rpx;
  background: #C15C3E;
  border-radius: 50%;
  font-size:44rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:120rpx;
  text-align: center;
  margin: 30rpx 0 40rpx 68rpx;
}
.box_border{
  width: 750rpx;
  height: 205rpx;
  position: relative;

}
.box_border image{
  float: right;
  width: 644rpx;
  height: 181rpx;
}
.box_border_delect{
  position: absolute;
  right: 101rpx;
  top: 40rpx;
  width: 30rpx;
  height: 30rpx;
}
.box_border_delect image{
  width: 30rpx;
  height: 30rpx;
}
.box_border_month{
  width: 126rpx;
  height: 1rpx;
  line-height: 178rpx;
  text-align: center;
  font-size: 30rpx;
  color:#9B9B9B;
}
.box_border_title{
  width:407rpx;
height:100rpx;

  line-height: 32rpx;
  font-size: 32rpx;
  color: #F5A623;
  position: absolute;
  left: 210rpx;
  top: 40rpx;
}
.box_border_content{
  width: 320rpx;
  height: 32rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  color: #9B9B9B;
  position: absolute;
  left: 210rpx;
  top: 110rpx;
}
.box_border_zizhen{
   width:407rpx;
height:100rpx;

  line-height: 32rpx;
