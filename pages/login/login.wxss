.eject{
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 200rpx;
  margin: auto;
  border-radius: 25rpx;
  z-index: 1;
  
}
.eject image{
  border: 1px solid #fff;
  width: 100%;
  height: 100%;
  opacity: 0.9;
  border-radius: 20rpx;
  
}
.register_img{
  width: 136rpx;
  height: 204rpx;
  left: 0;
  right: 0;
  margin: 30rpx auto;
}
.register_img image{
  width: 136rpx;
  height: 204rpx;
}
.register_phone,.register_yanzm,.register_invite{
  width: 670rpx;
  height: 100rpx;
  left: 0;
  right: 0;
  margin: 30rpx auto;
  border-bottom: 4rpx solid #f5f5f5;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.register_phone image{
  width: 34rpx;
  height: 47rpx;
  margin-right: 36rpx;
}
.register_yanzm image{
  width: 36rpx;
  height: 41rpx;
  margin-right: 35rpx;
  
}
.register_invite image{
  width: 47rpx;
  height: 47rpx;
  margin-right: 35rpx;
}
.register_phone input,.register_yanzm input ,.register_invite input{
  font-size: 28rpx;
  color: #9B9B9B;
}
.register_yanzm_gain{
  margin-left: 140rpx;
  font-size: 28rpx;
  color: #C15C3E;
}
.reqister_login{
  width: 640rpx;
  height: 80rpx;
  left: 0;
  right: 0;
  margin: 60rpx auto 30rpx;
  background:rgba(193,92,62,1);
  border-radius: 50rpx;
  font-size: 34rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
}
.reqister_hint{
  width: 660rpx;
  height: 80rpx;
  font-size: 22rpx;
  left: 0;
  right: 0;
  margin: 0 auto 128rpx;
  color: #666666;
}
.reqister_hint text{
  color: #C15C3E;
  line-height: 40rpx;
}
.reqister_toLogin{
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 180rpx;
  height: 50rpx;
  text-align: center;
  font-size: 30rpx;
  color: #C15C3E;
  
}